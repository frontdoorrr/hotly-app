# Development Environment Variables

# API
API_BASE_URL=http://localhost:8000/api/v1

# Firebase (개발용 - 실제 값으로 교체 필요)
FIREBASE_API_KEY=your_dev_firebase_api_key
FIREBASE_APP_ID=your_dev_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_dev_sender_id
FIREBASE_PROJECT_ID=hotly-dev

# Kakao Map API
KAKAO_MAP_API_KEY=your_dev_kakao_map_key

# Google OAuth
GOOGLE_CLIENT_ID=your_dev_google_client_id

# Apple Sign In
APPLE_CLIENT_ID=com.hotly.app.dev

# 기타 설정
ENABLE_LOGGING=true
LOG_LEVEL=DEBUG
SENTRY_DSN=
