# ===== Production Environment =====
ENVIRONMENT=production

# ===== Supabase Configuration =====
SUPABASE_URL=https://your-production-project.supabase.co
SUPABASE_ANON_KEY=your-production-anon-key-here

# OAuth Redirect (Deep Link)
REDIRECT_URL=hotly://login-callback

# ===== Backend API Configuration =====
API_BASE_URL=https://api.hotly.app/api/v1
API_TIMEOUT=30000

# ===== Kakao Map API =====
KAKAO_MAP_APP_KEY=YOUR_KAKAO_APP_KEY_PRODUCTION
KAKAO_MAP_JS_KEY=YOUR_KAKAO_JS_KEY_PRODUCTION

# ===== Firebase Configuration =====
FIREBASE_PROJECT_ID=hotly-app-prod

# ===== Feature Flags =====
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_DEBUG_LOGGING=false

# ===== App Configuration =====
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1

# ===== Social Login =====
GOOGLE_CLIENT_ID_IOS=your-google-client-id-ios-prod
GOOGLE_CLIENT_ID_ANDROID=your-google-client-id-android-prod
APPLE_CLIENT_ID=com.hotly.hotly_app.signin

# ===== Error Tracking =====
SENTRY_DSN=your-sentry-dsn-here

# ===== Logging =====
LOG_LEVEL=ERROR
ENABLE_CONSOLE_LOGGING=false
