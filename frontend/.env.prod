# Production Environment Variables

# API
API_BASE_URL=https://api.hotly.app/api/v1

# Firebase (프로덕션 - 실제 값으로 교체 필요)
FIREBASE_API_KEY=your_prod_firebase_api_key
FIREBASE_APP_ID=your_prod_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_prod_sender_id
FIREBASE_PROJECT_ID=hotly-prod

# Kakao Map API
KAKAO_MAP_API_KEY=your_prod_kakao_map_key

# Google OAuth
GOOGLE_CLIENT_ID=your_prod_google_client_id

# Apple Sign In
APPLE_CLIENT_ID=com.hotly.app

# 기타 설정
ENABLE_LOGGING=false
LOG_LEVEL=ERROR
SENTRY_DSN=your_sentry_dsn
